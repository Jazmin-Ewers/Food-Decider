<%- include('../partials/header') %>
  <table class="table">
    <thead>
      <tr>
        <th scope="col">Restaurant Name</th>
        <th scope="col">Location</th>
        <th scope="col">Cuisine</th>
        <th scope="col"></th>
      </tr>
    </thead>
    <% saved_restaurants.forEach(function(s) { %>
    <tbody>
        <tr>
          <form class="new-form" action="/saved-restaurants/<%= s._id %>?_method=DELETE" method="POST">
            <td> <input type = "hidden" name = "restaurant_name" value = "<%- s.restaurant_name %>"> <%= s.restaurant_name %></td>
            <td> <input type = "hidden" name = "address" value = "<%- s.address %>"> <%= s.address %></td>
            <td> <input type = "hidden" name = "cuisine" value = "<%- s.cuisine %>"> <%= s.cuisine %></td>
            <td> <input type="submit" value="DELETE"> </td>
          </form>
        </tr>
    <% }); %>
      </tbody>
    </table>

<%- include('../partials/footer') %>